{
  "require": {
    "craftcms/cms": "^3.5.14",
    "craftcms/redactor": "2.8.3",
    "fruitstudios/linkit": "1.1.11",
    "nystudio107/craft-retour": "3.1.42",
    "nystudio107/craft-seomatic": "3.3.22",
    "presseddigital/colorit": "1.0.9.3",
    "putyourlightson/craft-blitz": "3.6.11",
    "solspace/craft-freeform": "3.8.4",
    "solspace/craft3-calendar": "3.0.13",
    "spacecatninja/imager-x": "v3.2.4",
    "spicyweb/craft-embedded-assets": "2.4.1",
    "spicyweb/craft-neo": "2.8.12",
    "verbb/field-manager": "2.2.1",
    "verbb/icon-picker": "1.1.9",
    "verbb/navigation": "1.4.1",
    "verbb/super-table": "2.6.3",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}

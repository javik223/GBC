{
  "require": {
    "craftcms/cms": "3.5.16",
    "craftcms/redactor": "2.8.5",
    "doublesecretagency/craft-cpcss": "2.2.1",
    "mmikkel/cp-field-inspect": "1.2.3",
    "nystudio107/craft-imageoptimize": "1.6.20",
    "nystudio107/craft-minify": "1.2.10",
    "nystudio107/craft-seomatic": "3.3.26",
    "putyourlightson/craft-blitz": "3.6.12",
    "sebastianlenz/linkfield": "1.0.24",
    "spacecatninja/imager-x": "v3.2.6",
    "verbb/cloner": "1.2.2",
    "verbb/field-manager": "2.2.1",
    "verbb/navigation": "1.4.7",
    "verbb/super-table": "2.6.4",
    "vlucas/phpdotenv": "^3.4.0",
    "wrav/simplesharing": "1.0.8"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
